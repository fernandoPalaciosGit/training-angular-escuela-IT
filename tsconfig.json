{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@basics/*": [
        "src/app/website/basics/*"
      ],
      "@product-user-list/*": [
        "src/app/website/product-user-list/*"
      ],
      "@todo-list/*": [
        "src/app/website/todo-list/*"
      ],
      "@contact/*": [
        "src/app/website/contact/*"
      ],
      "@website-shared/*": [
        "src/app/website/shared/*"
      ],
      "@shares/*": [
        "src/app/shared/*"
      ],
      "@cms/*": [
        "src/app/cms/*"
      ],
      "@authentication/*": [
        "src/app/authentication/*"
      ],
      "@env/*": [
        "src/environments/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
